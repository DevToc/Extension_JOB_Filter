{
  "name": "Indeed Jobs Filter Tools",
  "version": "1.1.0",
  "manifest_version": 3,
  "description": "Chrome Extension for filter jobs!",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "filter.js"
      ],
      "run_at": "document_end"
    }
  ],
  "permissions": [
    "unlimitedStorage",
    "storage",
    "tabs",
    "activeTab"
  ],
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}